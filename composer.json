{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": {
        "ternobo-chat": {
            "type": "git",
            "url": "https://git.ternobo.info/Ternobo/ternobo-chat.git"
        },
        "ffmpeg-vidoe": {
            "type": "git",
            "url": "https://git.ternobo.info/soroosh_tanzadeh/ffmpeg-video.git"
        },
        "sms": {
            "type": "git",
            "url": "https://github.com/soroosh-tanzadeh/SmsirLaravel.git"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "artesaos/seotools": "^0.19.0",
        "bacon/bacon-qr-code": "^2.0",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.2",
        "google/apiclient": "^2.9",
        "guzzlehttp/guzzle": "^7.0.1",
        "htmlmin/htmlmin": "^8.0",
        "intervention/image": "^2.5",
        "ipecompany/smsirlaravel": "@dev",
        "ixudra/curl": "^6.21",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.7",
        "laravel/passport": "^10.1",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/html-to-markdown": "^5.0",
        "nojimage/twitter-text-php": "^3.1",
        "paquettg/php-html-parser": "^3.1",
        "persian-validator/national-code": "^1.0",
        "phpmailer/phpmailer": "^6.1",
        "pragmarx/google2fa": "^8.0",
        "pragmarx/recovery": "^0.2.1",
        "saeedpooyanfar/zarinpal": "^5.0",
        "shetabit/payment": "^4.0",
        "silviolleite/laravelpwa": "^2.0",
        "socialiteproviders/discord": "^4.1",
        "socialiteproviders/dribbble": "^4.1",
        "socialiteproviders/facebook": "^4.1",
        "socialiteproviders/github": "^4.1",
        "socialiteproviders/instagram": "^4.1",
        "socialiteproviders/instagram-basic": "^4.1",
        "socialiteproviders/spotify": "^4.1",
        "socialiteproviders/telegram": "^4.1",
        "socialiteproviders/twitch": "^5.3",
        "socialiteproviders/twitter": "^4.1",
        "socialiteproviders/yahoo": "^4.1",
        "socialiteproviders/youtube": "^4.1",
        "socialiteproviders/zoho": "^4.1",
        "soroosh/ternobo-chat": "@dev",
        "soroosh/ternobo-wire": "@dev",
        "spatie/laravel-backup": "^6.14",
        "spatie/laravel-permission": "^5.4",
        "squizlabs/php_codesniffer": "^3.6",
        "stevebauman/location": "^5.2",
        "symfony/process": "^5.2",
        "taylornetwork/laravel-username-generator": "^2.4",
        "tedivm/jshrink": "^1.3",
        "ternobo/ffmpeg-php": "@dev",
        "tgalopin/html-sanitizer": "^1.4",
        "vonage/client": "^2.4",
        "vyuldashev/laravel-openapi": "^1.4"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "laravel/dusk": "^6.9",
        "laravel/telescope": "^4.6",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}