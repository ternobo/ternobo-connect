{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": {
        "ternobo-chat": {
            "type": "git",
            "url": "https://git.ternobo.info/Ternobo/ternobo-chat.git"
        }
    },
    "require": {
        "php": "^7.3",
        "artesaos/seotools": "^0.19.0",
        "bacon/bacon-qr-code": "^2.0",
        "doctrine/dbal": "^3.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "google/apiclient": "^2.9",
        "guzzlehttp/guzzle": "^7.0.1",
        "htmlmin/htmlmin": "^8.0",
        "inertiajs/inertia-laravel": "^0.2.12",
        "intervention/image": "^2.5",
        "ixudra/curl": "^6.21",
        "jenssegers/agent": "^2.6",
        "jenssegers/mongodb": "^3.8",
        "juhlinus/depictr": "^2.0",
        "laravel/framework": "^8.0",
        "laravel/jetstream": "^1.3",
        "laravel/passport": "^10.1",
        "laravel/sanctum": "^2.6",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.0",
        "paquettg/php-html-parser": "^3.1",
        "persian-validator/national-code": "^1.0",
        "phpmailer/phpmailer": "^6.1",
        "pragmarx/google2fa": "^8.0",
        "pragmarx/recovery": "^0.1.0",
        "saeedpooyanfar/zarinpal": "^5.0",
        "shetabit/payment": "^4.0",
        "silviolleite/laravelpwa": "^2.0",
        "soroosh/ternobo-chat": "@dev",
        "soroosh/ternobo-wire": "@dev",
        "spatie/laravel-backup": "^6.14",
        "stevebauman/location": "^5.2",
        "taylornetwork/laravel-username-generator": "^2.4",
        "tedivm/jshrink": "^1.3"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "laravel/dusk": "^6.9",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
